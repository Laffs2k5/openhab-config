sitemap test_ir label="Testing IR stuff"
{
    Frame label="Computer room ir blaster  rm3 mini #1" {
        Switch
            item=IR_GF_xRoom_Broadlink_RM3Mini_command
            mappings=[
                SONY_W5500_VOLUME_DOWN_X3="-",
                SONY_W5500_VOLUME_DOWN_X155="--",
                SONY_W5500_VOLUME_UP_X3="+",
                SONY_W5500_VOLUME_UP_X155="++",
                SONY_W5500_MUTE_TOGGLE_X5="mute",
                SONY_W5500_THEATER_MODE_X5="th",
                SONY_W5500_INPUT_HDMI_3_X5="hdmi3",
                SONY_W5500_INPUT_HDMI_4_X5="hdmi4",
                SONY_W5500_POWER_ON_X5="on",
                SONY_W5500_POWER_OFF_X5="off"
                ]
    }
    Frame label="Computer room ir blaster rm3 mini #2" visibility=[Network_GF_ComputerRoom_IrBlaster=="OFF"] {
        Text
            item=Network_GF_ComputerRoom_IrBlaster
    }
    Frame label="Logitech Z-5500 rm3 mini #2" visibility=[Network_GF_ComputerRoom_IrBlaster=="ON"] {
        Switch
            item=IR_GF_ComputerRoom_Broadlink_RM3Mini_command
            label="Volume"
            mappings=[
                LOGITECH_Z5500_VOLUMEDOWN_X2="-",
                LOGITECH_Z5500_VOLUMEUP_X2="+",
                LOGITECH_Z5500_VOLUME_BURST_TO_ZERO="0",
                LOGITECH_Z5500_VOLUME_BURST_TO_MAX="max",
                LOGITECH_Z5500_MUTE="mute"
            ]
        Switch
            item=IR_GF_ComputerRoom_Broadlink_RM3Mini_command
            label="Input"
            mappings=[
                LOGITECH_Z5500_SOURCE_COAX="Coax",
                LOGITECH_Z5500_SOURCE_DIRECT="Direct",
                LOGITECH_Z5500_SOURCE_OPTICAL="Optical"
            ]
        Switch
            item=IR_GF_ComputerRoom_Broadlink_RM3Mini_command
            label="Power"
            mappings=[LOGITECH_Z5500_POWER_X2="power"]
    }
